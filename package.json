{
  "name": "@react_ru_bot/bot",
  "version": "1.0.0",
  "main": "index.ts",
  "description": "A chat bot for tg://@react_ru",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "ts-node --require dotenv/config index.ts",
    "dev": "nodemon",
    "test": "vitest run",
    "seed": "knex --knexfile knexfile.ts seed:run",
    "migrate": "knex --knexfile knexfile.ts migrate:up",
    "rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "migrate:make": "knex migrate:make $2 --knexfile knexfile.ts -x ts"
  },
  "keywords": [],
  "author": "YChebotaev",
  "dependencies": {
    "command-line-args": "^5.2.1",
    "dotenv": "^16.0.3",
    "knex": "^2.3.0",
    "msgpackr": "^1.7.2",
    "natural": "^5.2.3",
    "sqlite3": "^5.1.2",
    "telegraf": "^4.10.0",
    "terminal-kit": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/natural": "^5.1.1",
    "@types/node": "^18.8.3",
    "@types/terminal-kit": "^2.5.0",
    "fs-extra": "^10.1.0",
    "nodemon": "^2.0.20",
    "vitest": "^0.24.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-ru/react_ru_bot.git"
  },
  "bugs": {
    "url": "https://github.com/react-ru/react_ru_bot/issues"
  },
  "homepage": "https://github.com/react-ru/react_ru_bot#readme",
  "license": "GPL-3"
}
